{#
/**
 * @file instance.html.twig
 * Default theme implementation to present Instance data.
 *
 * This template is used when viewing Instance pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_instance()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('instance') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
